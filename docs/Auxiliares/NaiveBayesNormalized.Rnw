Numeradores[1,3] <- ProbCond[3,1]*ProbC[3]; Numeradores[1,4] <- ProbCond[4,1]*ProbC[4]
Numeradores[2,1] <- ProbCond[1,2]*ProbC[1]; Numeradores[2,2] <- ProbCond[2,2]*ProbC[2]
